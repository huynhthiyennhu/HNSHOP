@model List<HNSHOP.Dtos.Response.ProductTypeResDto>
@foreach (var item in Model)
{
	<div class="panel panel-default">
		<div class="panel-heading">
			<h4 class="panel-title">
				<a asp-controller="Home" asp-action="Index" asp-route-productTypeId="@item.Id">@item.Name</a>
			</h4>
		</div>
	</div>
}
